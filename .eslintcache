);\n  const notify = () => {\n    toast.error(props.msg, {\n      position: toast.POSITION.BOTTOM_CENTER,\n      autoClose: 3000,\n      // className:\"toast-error\"\n    });\n    setTimeout(() => {\n      setToastDispatch(true);\n    }, 3000);\n  };\n\n  useEffect(() => {\n    console.log(\"alert rendered\")\n    notify();\n    return;\n  }, []);\n  return (\n    <div>\n      {toastDispatch ? <Redirect to=\"/Login\" /> : null}\n      <ToastContainer />\n    </div>\n  );\n}\n","/home/meetshah/Desktop/E-learning/E-Learning-platform/src/component/Alert/WarningAlert.js",["237"],"import React, { useEffect } from \"react\";\nimport { ToastContainer, toast } from \"react-toastify\";\nimport \"react-toastify/dist/ReactToastify.css\";\n// import \"./Alert.css\"\nexport function WarningAlert(props) {\n  const notify = () => {\n    toast.warn(props.msg, {\n      position: toast.POSITION.TOP_CENTER,\n      autoClose: 3000,\n      // className:\"toast-error\"\n    });\n    \n  };\n\n  useEffect(() => {\n    console.log(\"alert rendered\")\n    notify();\n    return;\n  }, []);\n  return (\n    <div>\n      <ToastContainer />\n    </div>\n  );\n}\n","/home/meetshah/Desktop/E-learning/E-Learning-platform/src/axios-order.js",[],"/home/meetshah/Desktop/E-learning/E-Learning-platform/src/component/MyLearning/MyLearning.js",["238","239"],"import React, { Component } from \"react\";\n\nimport { Link } from \"react-router-dom\";\nimport * as actions from \"../../store/actions/index\";\nimport { connect } from \"react-redux\";\nimport PurchasedCourse from \"./PurchasedCourse\";\nimport { useAuth } from \"../../contexts/AuthContext\";\nimport \"./MyLearning.css\"\nclass MyLearning extends Component {\n  state={\n    length:0,\n  }\n  componentDidMount(){\n    // this.props.getPurchasedCourses(this.props.responseId)\n    this.props.getPurchasedCourses(this.props.userId)\n    console.log(\"normal\",this.props.learning);\n    // this.props.setPurchasedCourses(this.props.learning)\n    \n  }